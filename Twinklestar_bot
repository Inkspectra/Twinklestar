from discord.ext import commands
from discord.ext.commands Support Bot
import asyncio

bot commands.Bot(command prefix="!"
@bot.event
async def on_ready():
   print ("Let us begin! Oh! That is if you're ready of course!")
   print ("Now, my name is" + bot.user.name)
   print "OH... and my id is:" + (bot.user.id)
      
@bot.command(pass_context=true)
async def Twinkleref(etx):
   await bot.say("Oh! Well... I have a pelt made out of stars and a... questionable name. As for my history, I'll leave that a secret.")

   
